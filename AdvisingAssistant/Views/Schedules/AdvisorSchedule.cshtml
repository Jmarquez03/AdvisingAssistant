@model IEnumerable<Schedule>

@{
    ViewData["Title"] = "Student Schedule";
}

<h2>Student Schedule</h2>

<table class="table">
    <thead>
        <tr>
            <th>Course Name</th>
            <th>Location</th>
            <th>Time</th>
            <th>Final Grade</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var schedule in Model)
        {
            <tr>
                <td>@schedule.Course.Name</td>
                <td>@schedule.Location</td>
                <td>@schedule.Time</td>
            </tr>
        }
    </tbody>
</table>
